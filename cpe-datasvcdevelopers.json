            "Resource": [
                "arn:aws:athena:us-west-2:501246562447:workgroup/primary",
                "arn:aws:athena:us-west-2:501246562447:workgroup/primary/*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "athena:ListTableMetadata",
                "athena:ListDatabases",
                "athena:GetTableMetadata",
                "athena:GetDatabase",
                "athena:GetDataCatalog"
            ],
