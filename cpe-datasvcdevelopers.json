            "Action": [
                "glue:SearchTables",
                "glue:GetTables",
                "glue:GetDatabases",
                "glue:GetTable",
                "glue:GetDatabase",
                "glue:GetPartitions"
            ],
            "Resource": [
                "arn:aws:glue:us-west-2:501246562447:catalog",
                "arn:aws:glue:us-west-2:501246562447:database/*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "glue:GetTables",
                "glue:GetTable",
                "glue:GetPartitions",
                "glue:GetPartition",
                "glue:GetDatabases",
                "glue:GetDatabase",
                "glue:GetCatalogImportStatus",
                "glue:BatchGetPartition"
            ],
            "Resource": [
                "arn:aws:glue:us-west-2:393000359662:catalog",
                "arn:aws:glue:us-west-2:393000359662:database/*"
            ]
        },
