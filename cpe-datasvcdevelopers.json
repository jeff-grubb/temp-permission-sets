            "Sid": "AllowS3FullAccess",
            "Effect": "Allow",
            "Action": [
                "s3:*"
            ],
            "Resource": [
                "arn:aws:s3:::ds-lz-fox-adi-dev/*",
                "arn:aws:s3:::ds-lz-fox-adi-dev"
            ]
        }
