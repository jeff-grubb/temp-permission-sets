        },
        {
            "Action": [
                "cloudformation:DeleteStack",
                "s3:DeleteBucket",
                "s3:DeleteObject"
            ],
            "Resource": "*",
            "Effect": "Deny"
        },
        {
            "Action": [
                "organizations:*",
                "cloudtrail:DeleteTrail"
            ],
            "Resource": [
                "*"
            ],
            "Effect": "Deny"
        },
        {
